package principal;

import classes.Aluno;
import classes.Diretor;
import classes.Secretario;

public class TesttandoClassesfilhas {
	
	
	public static void main(String[] args) {
		
		Aluno aluno = new Aluno();
		aluno.setNome("Paulo");
		
		System.out.println(aluno);
		
		Diretor diretor = new Diretor();
		diretor.setNome("Fulano");
		diretor.setRegistroEducacao("65545454541");
		
		System.out.println(diretor);
		
		Secretario secretario = new Secretario();
		secretario.setNome("Ciglano");
		secretario.setNivelCargo("SENIO");
		
		
		System.out.println(secretario);
		
		
		
		System.out.println("Salário do Aluno é = "+ aluno.salario());
		System.out.println("Salário do Diretor é = "+ diretor.salario());
		System.out.println("Salário do Secretário é = "+ secretario.salario());
		
		
		Pessoa pessoa = new Aluno();
		
		
	}
	
	
	

}
